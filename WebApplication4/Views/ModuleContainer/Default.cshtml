@model WebApplication4.Models.ModuleContainerViewModel

@{ 
    var pmMgr = new Settings.Manager.PageModuleManager();
    var moduleMgr = new Settings.Settings();
}


@foreach (var obj in pmMgr.GetPageModules(Model.PageID))
{
    var moduleDefine = moduleMgr.getModuleInfo(obj.ModuleID);

    if (moduleDefine == null)
    {
        continue;
    }

    string ctlName = moduleDefine.Controller;
    string actName = moduleDefine.Action;


    @Html.Action(actName, ctlName, new { ModuleInstanceID = Model.PageModuleID })
}